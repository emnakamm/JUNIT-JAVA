pipeline {
    agent {
        docker {
            image 'maven:3.8.1-adoptopenjdk-11'

        }
    }

    stages {
        stage('Build') {
            steps {
            	bat 'mvn clean install -DskipTests'	
                
            }
        stage('Build Docker') {
            steps {
            	bat 'docker build -t emna/my_app'	
                
            }

        }
}
